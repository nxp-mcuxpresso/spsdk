# This is configuration file that is used for YAML diff widget for showing
# the difference between the user configuration and the default configuration.
template_yaml: workspace/bootimg_mimx9596_serial_downloader.yaml
user_cfg: inputs/bootimg_encrypted_mx95_serial_downloader.yaml
custom_command: "%! nxpimage bootable-image get-templates -f mimx9596 -o workspace --force"

comments:
  primary_image_container_set: path to our signed primary container set configuration/binary
  secondary_image_container_set: path to our signed secondary container set configuration/binary
  post_export: path to the directory where fuse scripts will be saved 